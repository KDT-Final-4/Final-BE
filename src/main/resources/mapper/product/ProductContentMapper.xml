<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.final_team4.finalbe.product.mapper.ProductContentMapper">

    <insert id="insert">
        INSERT INTO PRODUCT_CONTENT (
            PRODUCT_ID,
            CONTENT_ID
        ) VALUES (
            #{productId},
            #{contentId}
        )
    </insert>

</mapper>
