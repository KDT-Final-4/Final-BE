<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.final_team4.finalbe.trend.mapper.TrendMapper">

    <insert id="insert" parameterType="Trend" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO TREND (
              CATEGORY_ID,
              KEYWORD,
              SEARCH_VOLUME,
              CREATED_AT,
              SNS_TYPE
        ) VALUES (
              #{categoryId},
              #{keyword},
              #{searchVolume},
              #{createdAt},
              #{snsType}
        )
    </insert>

</mapper>
